<%- include("./partials/header") %>


  <div class="d-flex justify-content-center container" id="formContent">
    <form class="border border-dark p-5 w-75 p-3 rounded" action="/sessions/<%= session._id %>/usercode" method="POST">
      <div class="text-center">
        <p class="h4 mb-4">Enter your code</p>
      </div>
      <div class="form-group">
        <label>Access Code</label>
        <input type="text" class="form-control mb-4 input-sm" name="accesscode" placeholder="Enter the access code">
      </div>
      <div class="form-group">
        <label>Admin Code</label>
        <input type="text" class="form-control mb-4 input-sm" name="admincode" placeholder="Enter the admin code">
      </div>
      
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>

<%- include("./partials/footer") %>